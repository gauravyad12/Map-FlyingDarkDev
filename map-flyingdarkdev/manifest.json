{
    "name": "Map-FlyingDarkDev",
    "description": "A Google Map Lead Extractor",
    "version": "1.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "idle",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/bg.png",
        "32": "/images/bg.png",
        "48": "/images/bg.png"
    }
}